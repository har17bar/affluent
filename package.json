{
  "name": "affluentTest",
  "version": "1.0.0",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "watch": "tsc -w -p .",
    "build": "tsc -p .",
    "start": "node ./lib/cli",
    "doc": "node ./lib/cli --help",
    "format": "prettier --write src/**/*.{js,ts}"
  },
  "dependencies": {
    "@types/node": "^14.0.11",
    "clime": "^0.5.14",
    "typescript": "^3.9"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
